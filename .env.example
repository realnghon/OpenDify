# Dify API Keys Configuration
# Format: Comma-separated list of API keys
# These keys are used to authenticate with the Dify API service
DIFY_API_KEYS=app-xxxxxxxxxxxxxxxx,app-yyyyyyyyyyyyyyyy,app-zzzzzzzzzzzzzzzz

# Dify API Base URL
# The base URL for the Dify API service endpoint
DIFY_API_BASE="https://api.dify.example.com/v1"

# Conversation Memory Mode
# Select how conversation history is handled:
# 1: Append history messages to the current message (default)
# 2: Use zero-width character encoding for history
CONVERSATION_MEMORY_MODE=1

# Server Configuration
# Host and port settings for the local server
SERVER_HOST="127.0.0.1"
SERVER_PORT=8000

# OpenAI Compatible API Keys
# Valid API keys for OpenAI compatible endpoints
VALID_API_KEYS="sk-abc123,sk-def456"
